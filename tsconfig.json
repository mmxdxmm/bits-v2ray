{
	"compilerOptions": {
		"target": "es2021",
		"lib": [
			"es2021"
		],
		"jsx": "react",
		"module": "es2022",
		"moduleResolution": "node",
		"esModuleInterop": "true",
		"types": [
			"@cloudflare/workers-types",
			"@types/node",
		],
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": false,
		"sourceMap": false,
		"removeComments": false,
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
